<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yupfeg.sample.ui.MainActivity">

    <Button
        android:id="@+id/btn_main_base_use"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="基础使用"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        tools:ignore="HardcodedText"
        />

    <Button
        android:id="@+id/btn_main_replace_url"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="动态替换url"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_main_base_use"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        tools:ignore="HardcodedText"
        />

    <Button
        android:id="@+id/btn_download_apk"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="下载apk文件"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/btn_main_replace_url"
        android:layout_marginTop="10dp"
        android:layout_marginStart="15dp"
        tools:ignore="HardcodedText"
        />


</androidx.constraintlayout.widget.ConstraintLayout>