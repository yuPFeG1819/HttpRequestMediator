<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yupfeg.sample">

    <application
        android:name="com.yupfeg.sample.app.MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RemoteMediator">
        <activity android:name="com.yupfeg.sample.ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ui.normal.NormalUseActivity"/>
        <activity android:name=".ui.replace.ReplaceUrlActivity"/>
        <activity android:name=".ui.download.DownloadApkActivity"/>
    </application>

</manifest>